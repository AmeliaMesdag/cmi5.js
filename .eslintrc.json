{
    "env": {
        "browser": true
    },
    "extends": "eslint-config-rusticisoftware",
    "globals": {
        "TinCan": true,
        "URI": true,
        "XDomainRequest": true
    },
    "rules": {
        "no-var": "off",
        "no-invalid-this": "off",
        "no-param-reassign": "off",
        "no-magic-numbers": "off"
    }
}
